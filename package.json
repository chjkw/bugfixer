{
	"name": "bugfixer",
	"displayName": "Bugfixer",
	"description": "",
	"version": "0.7.0",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "bugfixer.run",
				"title": "Bugfixer 실행"
			},
			{
				"command": "bugfixer.diff",
				"title": "[BugFixer] File Open"
			},
			{
				"command": "bugfixer.showProgress",
				"title": "[BigFixer] Show Progress Page"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "bugfixer-result",
					"title": "Bugfixer 결과",
					"icon": "resources/icon/bug-solid.svg"
				}
			]
		},
		"views": {
			"bugfixer-result": [
				{
					"id": "bugfixerResult",
					"name": "Bugfixer 결과"
				}
			]
		},
		"menus": {
			"explorer/context": [
				{
					"command": "bugfixer.run"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"class-transformer": "^0.4.0",
		"es6-shim": "^0.35.6",
		"get-all-files": "^4.1.0",
		"globby": "^12.0.2",
		"parse-diff": "^0.8.1",
		"reflect-metadata": "^0.1.13"
	}
}
